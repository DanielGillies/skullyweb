<script>
	function loadScript (filename, async, secondscript) {
		async = async || true;
		secondscript = secondscript || false;
		var fileref=document.createElement('script')
		fileref.setAttribute("type","text/javascript")
		if (async) fileref.setAttribute("async", "");
		if (secondscript) fileref.setAttribute("onload", "loadScript('"+secondscript+"')");
		fileref.setAttribute("src", filename);
		document.getElementsByTagName("head")[0].appendChild(fileref)
	}
	var pagescript = 'static/js/index.js';
	window.addEventListener('load', function () {
		//loadScript('static/js/vendor.js');
		//loadScript('static/js/main.js');
		//loadScript(pagescript);
		loadScript('//fast.wistia.net/assets/external/E-v1.js', true);
	});
</script>
<script async src="static/js/vendor.js" onload="if (loadScript) {loadScript('static/js/main.js', true, pagescript)} else loadScript('static/js/main.js', false);"></script>
